{
    "name": "burned42/npradio",
    "description": "Now Playing Radio",
    "type": "project",
    "license": "",
    "require": {
        "php": "^8.0.0",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-zend-opcache": "*",
        "sensio/framework-extra-bundle": "^6.0",
        "sentry/sentry-symfony": "^4.0",
        "symfony/apache-pack": "^1.0",
        "symfony/cache": "5.2.*",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-client": "5.2.*",
        "symfony/string": "5.2.*",
        "symfony/twig-bundle": "^5.2",
        "symfony/yaml": "^5.2",
        "twbs/bootstrap": "^4.1.1"
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-rest": "^1.0.0",
        "codeception/module-symfony": "^1.0.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12.0",
        "phpstan/phpstan-symfony": "^0.12",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        },
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "copy-bootstrap": "cp vendor/twbs/bootstrap/dist/css/bootstrap.min.css* public/css/",
        "post-install-cmd": [
            "@auto-scripts",
            "@copy-bootstrap"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@copy-bootstrap"
        ],
        "lint-yaml": "bin/console lint:yaml --parse-tags *.yml .travis.yml config/ tests/",
        "lint-container": "bin/console lint:container",
        "lint-twig": "bin/console lint:twig --show-deprecations templates/",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no --ansi --allow-risky=yes --diff",
        "phpstan": "vendor/bin/phpstan analyse --ansi",
        "phpcs": "vendor/bin/phpcs",
        "codeception": "vendor/bin/codecept run --ansi",
        "codeception-coverage-xml": "@codeception --coverage --coverage-xml",
        "codeception-coverage-html": "@codeception --coverage --coverage-html",
        "update-check": "composer outdated --direct --strict --ansi",
        "run-checks": [
            "@lint-yaml",
            "@lint-container",
            "@lint-twig",
            "@php-cs-fixer",
            "@phpstan",
            "@phpcs",
            "@codeception",
            "@update-check"
        ],
        "sync-flex-recipes": "composer symfony:sync-recipes --force"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
